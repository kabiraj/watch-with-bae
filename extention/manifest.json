{
    "manifest_version": 3,
    "name": "WatchWithBae Video Detector",
    "version": "1.0",
    "description": "Detects videos on web pages.",
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}